{
  "name": "ExampleAndTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.7",
    "react-native-animatable": "^1.3.0",
    "react-native-material-ripple": "^0.8.0",
    "react-native-material-textfield": "^0.12.0",
    "react-native-vector-icons": "^6.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-native": "^0.57.0",
    "@types/react-test-renderer": "^16.0.2",
    "babel-jest": "23.6.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.50.0",
    "react-dom": "^16.5.2",
    "react-test-renderer": "16.6.1",
    "ts-jest": "^23.10.3",
    "typescript": "^3.2.1"
  },
  "jest": {
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|lottie-react-native|native-base|react-native-vector-icons|react-navigation|react-navigation-redux-helpers)/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/tests/Setup/test-shim.js",
      "<rootDir>/tests/Setup/test-setup.js",
      "<rootDir>/tests/Setup/test-preprocessor.js"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "cacheDirectory": ".jest/cache"
  }
}
